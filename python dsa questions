        "# 7Ô∏è‚É£ Maximum Sum of Subarray of Size K\n",
        "# Input: lst = [2, 1, 5, 1, 3, 2], k = 3\n",
        "# Output: 9\n",
        "# (subarray: 5+1+3)\n",
        "lst=[2,1,5,1,3,2]\n",
        "k=3\n",
        "max_sum=0\n",
        "window_sum=0\n",
        "left=0\n",
        "\n",
        "for right in range(len(lst)):\n",
        "  window_sum+=lst[right]\n",
        "\n",
        "  if right-left+1 == k:\n",
        "    max_sum=max(max_sum,window_sum)\n",
        "    window_sum-=lst[left]\n",
        "    left+=1\n",
        "print(max_sum)"
      ],
      "metadata": {
        "id": "LYpmJBh7X2fm",
        "outputId": "ba1dba26-2b4e-4b9c-87d7-ef1a63f28be7",
