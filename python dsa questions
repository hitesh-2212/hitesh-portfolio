        "# Input: [2,1,5,1,3,2], k=3\n",
        "# Output: 9\n",
        "lst=[2,1,5,1,3,2]\n",
        "k=3\n",
        "window_sum=sum(lst[:k])\n",
        "max_sum=window_sum\n",
        "\n",
        "for i in range(k,len(lst)):\n",
        "  window_sum+=lst[i]-lst[i-k]\n",
        "  max_sum=max(max_sum,window_sum)\n",
        "print(max_sum)"
