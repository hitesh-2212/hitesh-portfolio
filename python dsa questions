        "# Input:\n",
        "# lst = [2,7,11,15], target = 9\n",
        "# Output:\n",
        "# (2,7)\n",
        "lst=[2,7,11,15]\n",
        "target=9\n",
        "for i in range(len(lst)):\n",
        "  for j in range(i+1,len(lst)):\n",
        "    if lst[i]+lst[j]==target:\n",
        "      print((lst[i],lst[j]))"
