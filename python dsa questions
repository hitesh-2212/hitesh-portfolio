        "# employees(emp_id, name, dept_id, salary, join_date)\n",
        "# departments(dept_id, dept_name)\n",
        "# sales(order_id, emp_id, amount, order_date)\n",
        "\n",
        "\n",
        "# Return the second highest salary from employees.\n",
        "\n",
        "# Return each department and its average salary (rounded to 2 decimals)."
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "wuZToYS9cSdK"
      },
      "outputs": [],
      "source": [
        "select emp_id,name\n",
        "from (select *,dense_rank()over(order by salary desc)as rnk from employees)\n",
        "where rnk=2\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "foVw2rREcs8a"
      },
      "outputs": [],
      "source": [
        "select d.dept_name,round(avg(e.salary),2)as average_salary\n",
        "from employees as e\n",
        "join departments as d\n",
        "on e.dept_id=d.dept_id\n",
        "group by d.dept_name"
      ]
    },
