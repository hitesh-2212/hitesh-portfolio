        "# 1Ô∏è‚É£ Check if Two Strings Are Isomorphic\n",
        "# Input:\n",
        "# s = \"egg\", t = \"add\"\n",
        "# Output: True\n",
        "# (Each character in s can map to exactly one character in t)\n",
        "\n",
        "def is_isomorphic(s,t):\n",
        "  if len(s)!=len(t):\n",
        "    return False\n",
        "\n",
        "  map1={}\n",
        "  map2={}\n",
        "  for a,b in zip(s,t):\n",
        "    if a in map1 and map1[a]!=b:\n",
        "      return False\n",
        "    if b in map2 and map2[b]!=a:\n",
        "      return False\n",
        "    map1[a]=b\n",
        "    map2[b]=a\n",
        "  return True\n",
        "\n",
        "\n",
        "print(is_isomorphic('egg','add'))\n",
        ""
      ],
      "metadata": {
        "id": "aAZV4b8EhOaW",
        "outputId": "cc168ea0-b7ec-43c9-ad7c-7823fc5b9546",
        "colab": {
          "base_uri": "https://localhost:8080/"
        }
      },
      "execution_count": 19,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "True\n"
          ]
        }
      ]
    },
