        "# 1Ô∏è‚É£7Ô∏è‚É£ Check if two strings are isomorphic\n",
        "\n",
        "# Input: \"egg\", \"add\"\n",
        "# Output: True\n",
        "def is_isomorphic(s,l):\n",
        "  if len(s)!=len(l):\n",
        "    return False\n",
        "  map1={}\n",
        "  map2={}\n",
        "  for a,b in zip(s,l):\n",
        "    if a in map1 and map1[a]!=b:\n",
        "      return False\n",
        "    if b in map2 and map2[b]!=a:\n",
        "      return False\n",
        "    map1[a]=b\n",
        "    map2[b]=a\n",
        "  return True\n",
        "print(is_isomorphic('egg','add'))\n"
      ],
      "metadata": {
        "id": "QLOAh_CnBxMM",
